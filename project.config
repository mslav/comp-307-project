[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mark Slavsky (mslav)
[access "refs/for/refs/*"]
	read = group user/Mark Slavsky (mslav)
	push = group user/Mark Slavsky (mslav)
	label-Code-Review = -2..+2 group user/Mark Slavsky (mslav)
	label-Verified = -1..+1 group user/Mark Slavsky (mslav)
	rebase = group user/Mark Slavsky (mslav)
	removeReviewer = group user/Mark Slavsky (mslav)
	submit = group user/Mark Slavsky (mslav)
[access "refs/heads/*"]
	read = group user/Mark Slavsky (mslav)
	create = group user/Mark Slavsky (mslav)
	push = +force group user/Mark Slavsky (mslav)
	pushMerge = group user/Mark Slavsky (mslav)
[access "refs/tags/*"]
	push = group user/Mark Slavsky (mslav)
	push = +force group user/Mark Slavsky (mslav)
